{
  "name": "demo-vite-ts-shop",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "install:clean": "rm -rf node_modules && pnpm install",
    "install:update": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "dev": "vite",
    "build": "vite build && XR_ENV=avp vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
    "dev:avp": "dotenv -- webspatial-builder dev --manifest=public/manifest.webspatial.dev.json",
    "dev:all": "concurrently \"XR_ENV=avp pnpm dev\"  \"pnpm dev:avp \" ",
    "build:avp": "dotenv -- sh -c 'webspatial-builder build --manifest=public/manifest.webspatial.prod.json --project=dist --export=build/ --teamId=$XR_TEAM_ID'",
    "publish:avp": "dotenv -- sh -c 'webspatial-builder publish --manifest=public/manifest.webspatial.prod.json --project=dist --teamId=$XR_TEAM_ID --version=$XR_VERSION --u=$XR_DEV_NAME --p=$XR_DEV_PASSWORD'",
    "dev:internal": "XR_ENV=avp vite --config internal.vite.config.ts ",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@google/model-viewer": "4.0.0",
    "@webspatial/core-sdk": "latest",
    "@webspatial/react-plugin-vite": "latest",
    "@webspatial/react-sdk": "latest",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.3.0",
    "three": "^0.169.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "@types/react": "^19.0.11",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "@webspatial/builder": "^0.0.8",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.3",
    "typescript-eslint": "^8.26.1",
    "vite": "^6.2.2",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^0.21.1"
  }
}
